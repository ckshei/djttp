<h1>Event#show</h1>
<%= link_to("RSVP", :controller => :events, :action => :rsvp) unless @event.guests.include? current_user%>
<div class="js-event" data-id="<%= @event.id%>">
  
</div>
<% @event.guests.each do |guest| %>
  <%= guest.display_name %>
  <br>
<% end %>

<h1>Playlist Data</h1>
<table class="js-playlist-data">
  <tr>
      <th> Song Name </th>
      <th> Request Count </th>
    </tr> 
</table>
